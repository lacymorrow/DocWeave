		"react-shortcuts-hook": "^1.1.2",
		"react-syntax-highlighter": "^15.5.0",
		"react-use": "^17.5.1",
		"remark-breaks": "^4.0.0",
		"remark-containers": "^1.2.0",
		"remark-emoji": "^5.0.1",
		"remark-external-links": "^9.0.1",
		"remark-gfm": "^4.0.0",
		"remark-hint": "^1.0.10",
		"remark-mark-plus": "^1.0.21",
		"remark-slug": "^7.0.1",
		"remark-sub-super": "^1.0.21",
		"tinacms": "^2.1.0"
