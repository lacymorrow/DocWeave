{
	"name": "dev101",
	"url": "https://development-101.vercel.app",
	"version": "2.1.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git://github.com/aam-institute/dev101.git"
	},
	"scripts": {
		"dev": "tinacms dev -c \"next\"",
		"dev:debug": "NODE_OPTIONS='--inspect' next dev",
		"dev:watch": "next-remote-watch ./content",
		"postbuild": "next-sitemap --config next-sitemap-config.js",
		"build": "tinacms build && next build",
		"start": "tinacms build && next start",
		"export": "next build && next export",
		"lint": "next lint",
		"deploy": "tinacms build && next build && git commit -am 'deploy' && git push"
	},
	"dependencies": {
		"@mdx-js/mdx": "^3.0.1",
		"@mdx-js/react": "^3.0.1",
		"@next-auth/mongodb-adapter": "^1.1.3",
