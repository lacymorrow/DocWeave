		"@tinacms/cli": "^1.5.52",
		"classnames": "^2.5.1",
		"github-slugger": "^2.0.0",
		"gray-matter": "^4.0.3",
		"markdown-toc": "^1.2.0",
		"mdx-embed2": "^2.0.1",
		"next": "^14.2.5",
		"next-auth": "^4.24.7",
		"next-compose-plugins": "^2.2.1",
		"next-mdx-remote": "^5.0.0",
		"next-remote-watch": "2.0.0",
		"next-seo": "^6.5.0",
		"next-sitemap": "^4.2.3",
		"nodemailer": "^6.9.14",
		"prettier": "^3.3.3",
